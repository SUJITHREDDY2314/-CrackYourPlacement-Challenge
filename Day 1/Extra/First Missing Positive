class Solution:
    def firstMissingPositive(self, nums: List[int]) -> int:
        nums=list(set(filter(lambda x:x>0 , nums)))
        nums.sort()
        if(nums==[] or nums[0]!= 1):
            return 1
        for i in range(len(nums)-1):
            if nums[i+1]!=nums[i]+1:
                return nums[i]+1
        return nums[-1]+1
        
