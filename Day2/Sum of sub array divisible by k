
class Solution:
    def subarraysDivByK(self, nums: List[int], k: int) -> int:

        ans=0
        prefsum=0
        d={0:1}
        for num in nums:
            prefsum = prefsum + num
            if prefsum%k in d:
                ans = ans + d[prefsum%k]
            if prefsum%k not in d:
                d[prefsum%k] = 1
            else:
                d[prefsum%k]+=1
        return ans
